        Fecha: 30/01/2024
        Description: programa que crea un objeto
"""

class Alumno:#define la clase
    matricula=None#atributos
    nombre=None#atributos
    def __init__(self,matricula,nombre):#constructor
        self.matricula=matricula#atributos
        self.nombre=nombre#atributos
        print("Objeto Alumno")#imprime el objeto
    def estudiar(self):
        print(f"{self.nombre} esta estudiando")
    def sumar(self,numero1,numero2):
        suma=numero1+numero2
        print(f"la suma es: {suma}")
      
deja=Alumno("1111","Deja")
deja.estudiar()
deja.sumar(10,5)
print(f"El nombre es: {deja.nombre}")
print(f"La matricula es: {deja.matricula}")
